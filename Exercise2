#include <avr/io.h>

int main(void){
        DDRB = 0xFF; // Configure port B's 8 pins as outputs
        PORTA = 0x00;
        PORTC = 0x00;
        Light = 0x01;

        while(1)
{
        if(PORTA0||PORT1||PORT2||PORT3 = 1||2)
        {
        PORTC5 && PORT6 =Light;
        scanf("Low fuel");
        }

        if(PORTA0||PORT1||PORT2||PORT3  = 3||4)
        {
        PORTC5 && PORTC6 && PORTC4 =Light;
        scanf("Low fuel");
        }

        if(PORTA0||PORT1||PORT2||PORT3  = 5||6)
        {
        PORT5 && PORT4 && PORT3 =Light;
        }

        if(PORTA0||PORT1||PORT2||PORT3  = 7||8||9)
        {
        PORT5 && PORT4 && PORT3 && PORT2 =Light;
        }

        if(PORTA0||PORT1||PORT2||PORT3  = 10||11||12)
        {
        PORT5 && PORT4 && PORT3 && PORT2 && PORT1 =Light;
        }

        if(PORTA0||PORT1||PORT2||PORT3  = 13||14||15)
        {
        PORT5 && PORT4 && PORT3 && PORT2 && PORT1 && PORT0=Light;
        }

        return 1;
}




